# Environment variables for Cart Protocol EVM contracts

# Deployment (REQUIRED for deploying)
PRIVATE_KEY=your_private_key_here

# Fee Configuration
PROTOCOL_FEE_RECIPIENT=0x_your_protocol_fee_recipient_address
ARBITRATION_FEE_RECIPIENT=0x_your_arbitration_fee_recipient_address

# RPC URLs (optional, will use defaults if not set)
BASE_SEPOLIA_RPC=https://sepolia.base.org
BASE_RPC=https://mainnet.base.org

# Block Explorer API Keys (for verification)
BASESCAN_API_KEY=your_basescan_api_key

# Deployed Contract Addresses (filled after deployment)
TRUST_ENGINE_ADDRESS=
IDENTITY_REGISTRY_ADDRESS=
REPUTATION_REGISTRY_ADDRESS=
VALIDATION_BRIDGE_ADDRESS=
GATEWAY_SESSION_ADDRESS=

# Multi-Party E2E Testing (optional - for realistic fund flow testing)
# If not set, E2E tests use PRIVATE_KEY as both agent and provider
PRIVATE_KEY_AGENT=    # Agent wallet - opens sessions, pays for API usage
PRIVATE_KEY_PROVIDER= # Provider wallet - registers gateways, receives payments
